<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="moe.feo.littlebot.fxui.Controller">
   <children>
      <TabPane fx:id="tabPane" layoutX="14.0" layoutY="14.0" onMousePressed="#stateChanged" prefHeight="40.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
      </TabPane>
      <TextArea fx:id="historyTextArea" editable="false" layoutX="14.0" layoutY="38.0" prefHeight="430.0" prefWidth="580.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="60.0">
         <font>
            <Font size="16.0" />
         </font></TextArea>
      <TextArea fx:id="inputTextArea" layoutX="10.0" layoutY="470.0" onKeyPressed="#keyPressed" onKeyReleased="#keyReleased" prefHeight="100.0" prefWidth="420.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
         <font>
            <Font size="16.0" />
         </font>
      </TextArea>
      <Button fx:id="sendButton" mnemonicParsing="false" onAction="#send" prefHeight="100.0" prefWidth="158.0" text="发送" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <font>
            <Font size="16.0" />
         </font>
      </Button>

   </children>
</AnchorPane>
